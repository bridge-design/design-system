{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:mdx/recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "overrides": [
    {
      "files": "*.mdx",
      "parser": "eslint-mdx"
    }
  ]
}
